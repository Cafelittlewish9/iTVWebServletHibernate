<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="model.vo.FollowVO" table="Follow">
		<composite-id>
			<key-property name="memberId" column="memberId" />
			<key-property name="followId" column="followId" />
		</composite-id>
		<many-to-one name="member" class="model.vo.MemberVO" lazy="false">
			<column name="Follow.followId" />
		</many-to-one>
	</class>
</hibernate-mapping>